   "id": "10244ae4-17d4-4502-9680-553895691648",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "N = 22\n",
    "m = 20"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "ac1cc73f-60ce-440c-8b16-b94d47fc6c30",
   "metadata": {},
   "outputs": [],
   "source": [
    "A = np.eye(20, 20) * (N*m + m/N)"
   ]
  }
